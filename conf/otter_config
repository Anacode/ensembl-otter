
[client]
port=33999
host=humsrv1
write_access=1


[default.filters]
genscan=1
trf=1
swall=1
est2Genome_other=1
repeatmask=1
est2genome_human=1
eponine=1
refseq_human=1
halfwise=1
cpg=1
fgenesh=1
est2genome_fish=1
est2genome_mouse=1
vertrna=1

[human.filters]
est2genome_fish=0

[zebrafish.filters]
est2genome_human=0
est2genome_mouse=0
refseq_human=0

[default.filter.RepeatMask]
module=Bio::EnsEMBL::Ace::Filter::Repeatmasker

[default.filter.trf]
module=Bio::EnsEMBL::Ace::Filter::TRF

[default.filter.Est2Genome_human]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
hseq_prefix=Em:

[default.filter.Est2Genome_fish]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
hseq_prefix=Em:

[default.filter.Est2Genome_mouse]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
hseq_prefix=Em:

[default.filter.Est2Genome_other]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
hseq_prefix=Em:

[default.filter.vertrna]
module=Bio::EnsEMBL::Ace::Filter::Similarity::DnaSimilarity
max_coverage=10
hseq_prefix=Em:

[default.filter.genscan]
module=Bio::EnsEMBL::Ace::Filter::Gene::Predicted

[default.filter.Fgenesh]
module=Bio::EnsEMBL::Ace::Filter::Gene::Predicted

[default.filter.CpG]
module=Bio::EnsEMBL::Ace::Filter::CpG

[default.filter.Halfwise]
module=Bio::EnsEMBL::Ace::Filter::Gene::Halfwise
url_string=http:\/\/www.sanger.ac.uk\/cgi-bin\/Pfam\/getacc?%s

[default.filter.refseq_human]
module=Bio::EnsEMBL::Ace::Filter::Similarity::RefSeq
# This url requires a LocusLink ID
# http://www.ncbi.nlm.nih.gov/LocusLink/help.html#LinkingtoaSpecificRecord
# url_string=http\:\/\/www.ncbi.nlm.nih.gov\/LocusLink\/LocRpt.cgi?l=?%s
########################################
# This url does a search as described by
# http://www.ncbi.nlm.nih.gov/LocusLink/help.html#CreatingaLink
url_string=http:\/\/www.ncbi.nlm.nih.gov\/LocusLink\/list.cgi?ORG\=Hs\&Q\=%s

[default.filter.swall]
module=Bio::EnsEMBL::Ace::Filter::Similarity::ProteinSimilarity
percent_identity_cutoff=40

[default.filter.eponine]
module=Bio::EnsEMBL::Ace::Filter::SimpleFeature


