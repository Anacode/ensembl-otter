#!/bin/tcsh

setenv OTTER_HOME /Users/michele/cvs/ensembl-otter/dist

setenv PATH ${OTTER_HOME}/bin:${OTTER_HOME}/ensembl-otter/scripts:${OTTER_HOME}/ensembl-otter/scripts/server:{$PATH}

mysqladmin -u root -h 127.0.0.1 -P3377 shutdown

foreach i (`ps aux |grep otter_srv |grep -v grep |awk '{print $2}'`)
  kill $i
end
